    "            if col == 'log_amount':     cell.font = Font(name='Arial', bold=True, size=9, color='7B241C')\n",
    "\n",
    "        ws.row_dimensions[er].height = row_height\n",
    "\n",
    "    if freeze: ws.freeze_panes = freeze\n",
    "\n",
    "print('write_sheet helper defined ✅')"
