    "# Map new feature → its group for colour-coding\n",
    "COL_TO_GROUP = {row[1]: row[0] for row in FEATURE_GUIDE}\n",
    "\n",
    "def thin_border(color):\n",
    "    s = Side(style='thin', color=color)\n",
    "    return Border(left=s, right=s, top=s, bottom=s)\n",
    "\n",
    "BORDER = thin_border(C['border'])\n",
    "\n",
    "def write_sheet(ws, data_df, freeze='D2', row_height=18):\n",
    "    \"\"\"Write a DataFrame to a worksheet with full styling.\"\"\"\n",
    "    cols = data_df.columns.tolist()\n",
    "\n",
    "    # Header\n",
    "    for ci, col in enumerate(cols, 1):\n",
    "        cell = ws.cell(row=1, column=ci, value=col)\n",
    "        cell.font      = Font(name='Arial', bold=True, color=C['header_fg'], size=10)\n",
    "        cell.fill      = PatternFill('solid', fgColor=C['header'])\n",
    "        cell.alignment = Alignment(horizontal='center', vertical='center', wrap_text=True)\n",
    "        cell.border    = BORDER\n",
    "    ws.row_dimensions[1].height = 32\n",
    "\n",
    "    # Data\n",
    "    prev_year = None\n",
    "    for ri, row in data_df.iterrows():\n",
    "        er       = list(data_df.index).index(ri) + 2  # sequential excel row\n",
    "        is_new   = ('year' in data_df.columns and row.get('year') != prev_year)\n",
    "        if is_new and 'year' in data_df.columns:\n",
    "            prev_year = row.get('year')\n",
    "\n",
    "        for ci, col in enumerate(cols, 1):\n",
    "            val = row[col]\n",
    "            try:\n",
    "                if pd.isna(val): val = None\n",
    "            except Exception:\n",
    "                if str(val) in ('nan','NaN','<NA>'): val = None\n",
    "\n",
    "            grp = COL_TO_GROUP.get(col)\n",
    "            if is_new:         bg = C['year_band']\n",
    "            elif grp:          bg = GROUP_BG.get(grp, 'FFFFFF')\n",
    "            elif ri % 2 == 0:  bg = C['alt']\n",
    "            else:              bg = 'FFFFFF'\n",
    "\n",
    "            cell = ws.cell(row=er, column=ci, value=val)\n",
    "            cell.fill      = PatternFill('solid', fgColor=bg)\n",
    "            cell.alignment = Alignment(horizontal='center', vertical='center')\n",
    "            cell.border    = BORDER\n",
    "            cell.font      = Font(name='Arial', size=9)\n",
    "\n",
    "            if col == 'player_id':      cell.font = Font(name='Arial', bold=True, size=9, color=C['gold'])\n",
